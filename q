[32m+[m[32m/**[m
[32m+[m[32m * Created by llllou on 2017/6/13 0013.[m
[32m+[m[32m */[m
[32m+[m[32mvar small=document.getElementById("small");[m
[32m+[m[32mvar lun=document.getElementById("lun");[m
[32m+[m[32mvar big=document.getElementById("big");[m
[32m+[m[32mvar bigImg=big.getElementsByTagName("img")[0];[m
[32m+[m[32mvar smallImg=small.getElementsByTagName("img")[0];[m
[32m+[m[32mvar img=document.getElementsByClassName("show")[0];[m
[32m+[m[32mevent=event||window.event;[m
[32m+[m[32mimg.onclick=function(event)[m
[32m+[m[32m{[m
[32m+[m[32m    var container="";[m
[32m+[m[32m    var target=event.target||event.srcElement;[m
[32m+[m[32m    if(target.nodeName.toLowerCase()=="img"){[m
[32m+[m[32m        container=target.src;[m
[32m+[m[32m        container=container.replace(/(50x64)/,"350x449");[m
[32m+[m[32m        smallImg.src=container;[m
[32m+[m[32m        container=target.src.replace(/(50x64)/,"800x1026");[m
[32m+[m[32m        bigImg.src=container;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m[32mfunction getAllTop(obj){[m
[32m+[m[32m    var current=obj;[m
[32m+[m[32m    var alltop=obj.offsetTop;[m
[32m+[m[32m    while(current=current.offsetParent)[m
[32m+[m[32m    {[m
[32m+[m[32m        alltop+=current.offsetTop;[m
[32m+[m[32m    }[m
[32m+[m[32m    return alltop;[m
[32m+[m[32m}[m
[32m+[m[32msmall.onmouseover=function(){[m
[32m+[m[32m    lun.style.display="block";[m
[32m+[m[32m    big.style.display="block";[m
[32m+[m[32m}[m
[32m+[m[32msmall.onmousemove=function (event) {[m
[32m+[m[32m    var rate=800/350;[m
[32m+[m[32m    var st=document.body.scrollTop||document.documentElement.scrollTop;[m
[32m+[m[32m    var x=event.clientX- small.offsetLeft-0.5*lun.offsetWidth;[m
[32m+[m[32m    var y=event.clientY-(getAllTop(small)-st)-0.5*lun.offsetHeight;[m
[32m+[m[32m    if(x>=small.offsetWidth-lun.offsetWidth)[m
[32m+[m[32m    {[m
[32m+[m[32m        x=small.offsetWidth-lun.offsetWidth[m
[32m+[m[32m    }[m
[32m+[m[32m    else if(x<=0)x=0;[m
[32m+[m[32m    if (y>=small.offsetHeight-lun.offsetHeight)[m
[32m+[m[32m    {[m
[32m+[m[32m        y=small.offsetHeight-lun.offsetHeight[m
[32m+[m[32m    }[m
[32m+[m[32m    else if(y<=0)y=0;[m
[32m+[m[32m    lun.style.top=y+"px";[m
[32m+[m[32m    lun.style.left=x+"px";[m
[32m+[m[32m    bigImg.style.top=(-1*y)*rate+"px";[m
[32m+[m[32m    bigImg.style.left=(-1*x)*rate+"px";[m
[32m+[m[32m}[m
[32m+[m[32msmall.onmouseout=function(){[m
[32m+[m[32m    big.style.display="none";[m
[32m+[m[32m    lun.style.display="none";[m
[32m+[m[32m}[m
\ No newline at end of file[m
